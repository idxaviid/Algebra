Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"BinarySearch", "[", 
    RowBox[{"p_", ",", "a_", ",", "b_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"j", ",", "A", ",", "B", ",", "PM"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"j", ":=", "1"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"p", "[", "a", "]"}], "<", "0"}], ",", 
        RowBox[{"A", ":=", 
         RowBox[{"{", "a", "}"}]}], ",", 
        RowBox[{"A", ":=", 
         RowBox[{"{", "b", "}"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"p", "[", "b", "]"}], ">", "0"}], ",", 
        RowBox[{"B", ":=", 
         RowBox[{"{", "b", "}"}]}], ",", 
        RowBox[{"B", ":=", 
         RowBox[{"{", "a", "}"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"PM", ":=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"a", "+", "b"}], ")"}], "/", "2"}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"j", "<", "n"}], ",", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"A", ",", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"p", "[", 
               RowBox[{"PM", "[", 
                RowBox[{"[", "j", "]"}], "]"}], "]"}], "<", "0"}], ",", 
             RowBox[{"PM", "[", 
              RowBox[{"[", "j", "]"}], "]"}], ",", 
             RowBox[{"A", "[", 
              RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}], "]"}], ";", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"B", ",", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"p", "[", 
               RowBox[{"PM", "[", 
                RowBox[{"[", "j", "]"}], "]"}], "]"}], ">", "0"}], ",", 
             RowBox[{"PM", "[", 
              RowBox[{"[", "j", "]"}], "]"}], ",", 
             RowBox[{"B", "[", 
              RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}], "]"}], ";", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"PM", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"A", "[", 
               RowBox[{"[", 
                RowBox[{"j", "+", "1"}], "]"}], "]"}], "+", 
              RowBox[{"B", "[", 
               RowBox[{"[", 
                RowBox[{"j", "+", "1"}], "]"}], "]"}]}], ")"}], "/", "2"}]}], 
          "]"}], ";", 
         RowBox[{"j", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "PM", "]"}], ";"}]}], "]"}]}], ";"}]], "Input",
